parameters:
    level: 5
    paths:
        - src
        - tests
    excludePaths:
        - vendor/*
    ignoreErrors:
        - '#Call to an undefined method Illuminate\\Database\\Eloquent\\Builder::(withTrashed|onlyTrashed)#'
        - '#Call to function property_exists\(\) with .* will always evaluate to true#'
        - '#Call to function method_exists\(\) with .* will always evaluate to false#' 