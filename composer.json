{
    "name": "soliudeen999/laravel-simple-query-filter",
    "description": "A powerful and flexible query filter system for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Soliudeen999",
            "email": "soliudeen999@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.68",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Soliudeen999\\QueryFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soliudeen999\\QueryFilter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soliudeen999\\QueryFilter\\Providers\\QueryFilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
